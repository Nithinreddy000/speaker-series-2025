{
	"info": {
		"_postman_id": "962c4c4f-958a-447f-ad4d-00cbed1dcce5",
		"name": "Azure Semantic Search - Auto Embedding Copy",
		"description": "Automatically generates embeddings and uploads documents to Azure AI Search",
		"schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json",
		"_exporter_id": "35373448"
