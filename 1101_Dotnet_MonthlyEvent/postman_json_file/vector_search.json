		{
			"name": "6. Single Vector Search",
			"event": [
				{
					"listen": "prerequest",
					"script": {
						"exec": [
							"// Parse the query_embedding variable and inject it into the request body",
							"let embeddingString = pm.collectionVariables.get('query_embedding');",
							"",
							"if (embeddingString) {",
							"    try {",
							"        // Parse the JSON string to get the actual array",
							"        let embeddingArray = JSON.parse(embeddingString);",
							"        ",
							"        // Build the complete request body with the embedding array",
							"        let requestBody = {",
							"            \"search\": \"*\",",
							"            \"vectorQueries\": [",
							"                {",
							"                    \"kind\": \"vector\",",
							"                    \"vector\": embeddingArray,",
							"                    \"fields\": \"contentVector\",",
							"                    \"k\": 3",
							"                }",
							"            ]",
							"        };",
							"        ",
							"        // Set the request body",
							"        pm.request.body.raw = JSON.stringify(requestBody);",
							"        ",
							"        console.log('✅ Query embedding injected into request body');",
							"        console.log('Embedding array length:', embeddingArray.length);",
							"    } catch (e) {",
							"        console.error('❌ Error parsing query_embedding:', e);",
							"        console.error('Current value:', embeddingString);",
							"    }",
							"} else {",
							"    console.error('⚠️ query_embedding variable is not set. Please run \"5. Generate Query Embedding\" first.');",
							"}"
						],
						"type": "text/javascript",
						"packages": {},
						"requests": {}
					}
				}
			],
			"request": {
				"method": "POST",
				"header": [
					{
						"key": "Content-Type",
						"value": "application/json"
					},
					{
						"key": "api-key",
						"value": "{{search-api-key}}"
					}
				],
				"body": {
					"mode": "raw",
					"raw": "{\n  \"vectorQueries\": [\n    {\n      \"vector\": {{query_embedding}},\n      \"fields\": \"contentVector\",\n      \"k\": 3\n    }\n  ]\n}"
				},
				"url": {
					"raw": "https://{{search-service-name}}.search.windows.net/indexes/{{index-name}}/docs/search?api-version={{search-api-version}}",
					"protocol": "https",
